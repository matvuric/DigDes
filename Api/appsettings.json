{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSql": POSTGRESQL
  },
  "Auth": {
    "Issuer": "DigDes",
    "Audience": "mobileApp",
    "Key": AUTH_KEY,
    "LifeTime": 1
  },
  "PushConfig": {
    "Google": {
      "ServerKey": GOOGLE_SERVER_KEY,
      "SenderId": GOOGLE_SENDER_ID,
      "GcmUrl": "https://fcm.googleapis.com/fcm/send"
    }
  }
}
